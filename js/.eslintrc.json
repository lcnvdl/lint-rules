{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "linebreak-style": "off",
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ]
  }
}